    "bcrypt": "^5.1.1",
    "bullmq": "^5.4.7",
    "dotenv": "^16.4.5",
    "fastify": "^5.0.0",
    "ioredis": "^5.3.2",
    "minio": "^7.1.0",
    "sharp": "^0.32.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^22.7.4",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.2",
    "typescript": "^5.3.3"
